{
  "name": "normanhuth/nova-valuestore-settings",
  "description": "A Laravel Nova tool for editing custom settings using native Nova fields.",
  "keywords": [
    "laravel",
    "nova",
    "spatie",
    "valuestore"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Norman Huth",
      "email": "norman@huth.it",
      "homepage": "https://huth.it"
    },
    {
      "name": "Tarvo Reinpalu"
    }
  ],
  "require": {
    "php": ">=7.1.0",
  },
  "autoload": {
    "psr-4": {
      "NormanHuth\\NovaValuestore\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "NormanHuth\\NovaValuestore\\ToolServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
